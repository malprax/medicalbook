<!DOCTYPE html>
<html>
  <head>
    <title>Medicalbook</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="top-navigation">
    <%= yield :stylesheets %>
    <div id="wrapper">
      <div id="page-wrapper" class="gray-bg">
        <%= render 'layouts/inspinia-template/header' %>
        <div
  class="fb-like"
  data-share="true"
  data-width="450"
  data-show-faces="true">
</div>
        <%= yield %>
      </div>
    </div>
    <%= render 'layouts/inspinia-template/footer' %>
    <%= yield :javascripts %>
  </body>
</html>
